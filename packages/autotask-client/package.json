{
  "name": "@openzeppelin/defender-autotask-client",
  "version": "1.54.6",
  "description": "Client library for managing Defender Autotasks",
  "bin": {
    "defender-autotask": "./lib/bin.js"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
