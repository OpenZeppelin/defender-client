    "dotenv": "^10.0.0",
    "glob": "^7.1.6",
    "jszip": "^3.5.0",
    "lodash": "^4.17.19",
    "node-fetch": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenZeppelin/defender-client.git"
  },
